{
    "name": "stevebauman/location",
    "description": "Retrieve a users location by their IP Address",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license" : "MIT",
    "keywords" : [
        "laravel",
        "php",
        "location",
        "geo",
        "geoIP",
        "geo-location",
        "ip"
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/support": "~5.0|~6.0|~7.0",
        "geoip2/geoip2": "2.8.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~3.2|~4.0"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Location\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevebauman\\Location\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Stevebauman\\Location\\LocationServiceProvider"
            ],
            "aliases": {
                "Location": "Stevebauman\\Location\\Facades\\Location"
            }
        }
    }
}
